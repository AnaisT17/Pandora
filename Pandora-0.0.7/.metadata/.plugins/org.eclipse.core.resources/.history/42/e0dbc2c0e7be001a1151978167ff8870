package fr.seedle.pandora.interfaces;

import java.util.Set;

public interface Equipe extends Pandora {

	public String getBlason();

	public void setBlason(String blason);

	public String getLocalisation();

	public void setLocalisation();

	public Set<Membre> getListMembres();

	public void setListMembres(Set<Membre> listMembres);

	public String getDescription();

	public void setDescription(String description);

}
