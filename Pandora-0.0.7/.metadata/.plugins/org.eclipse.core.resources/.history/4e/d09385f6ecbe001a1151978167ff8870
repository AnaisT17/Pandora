package fr.seedle.pandora.interfaces;

import java.util.Set;

public interface RelationFactionEquipe extends Pandora {

	public Faction getFaction();

	public void setFaction(Faction faction);

	public Equipe getEquipe();

	public void setEquipe();

	public OperationRole getOperationRole();

	public void setOperationRole(OperationRole operationRole);

	public Set<Membre> getListMembres();

	public void setListMembres(Set<Membre> listMembre);
}
