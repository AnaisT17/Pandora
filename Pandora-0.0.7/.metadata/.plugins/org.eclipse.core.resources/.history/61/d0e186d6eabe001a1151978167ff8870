package fr.seedle.pandora.interfaces;

import java.time.LocalDateTime;
import java.util.Set;

public interface Operation extends Pandora {

	public String getDescription();

	public void setDescription(String description);

	public Lieu getLieu();

	public void setLieu(Lieu lieu);

	public LocalDateTime getDateDebut();

	public void setDateDebut(LocalDateTime dateDebut);

	public LocalDateTime getDateFin();

	public void setDateFin(LocalDateTime dateFin);

	public Equipe getEquipeOrganisatrice();

	public void setEquipeOrganisatrice(Equipe equipe);

	public Set<Faction> getListFactions();

	public void setListFactions(Set<Faction> listFactions);

}
